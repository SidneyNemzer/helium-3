{
  "include": ["./server/**/*", "dist/server.d.ts", "./client2/**/*"],
  "compilerOptions": {
    "lib": [
      "es2019",
      "es2020.promise",
      "es2020.bigint",
      "es2020.string",
      "dom"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2019",
    "noImplicitAny": true,
    "strict": true,
    "outDir": "build",
    "baseUrl": ".",
    "allowJs": true,
    "jsx": "react"
  }
}
